{
  "name": "kayorama-autopilot",
  "displayName": "Kayorama Autopilot",
  "description": "Automatically accept AI code suggestions from GitHub Copilot, Supermaven, and Chat. Features Native Mode (Zero-Config) and Pro CDP Mode.",
  "publisher": "Kayorama",
  "version": "8.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaygaaf/KayoAutopilot.git"
  },
  "keywords": [
    "ai",
    "automation",
    "copilot",
    "github copilot",
    "supermaven",
    "chat",
    "auto-accept",
    "ghost text",
    "inline completion",
    "suggestion",
    "productivity",
    "coding assistant",
    "workflow",
    "accept all"
  ],
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Machine Learning",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kayorama.toggle",
        "title": "Kayorama: Toggle Autopilot"
      },
      {
        "command": "kayorama.dumpDiagnostics",
        "title": "Kayorama: Dump Diagnostics"
      },
      {
        "command": "kayorama.dumpCommands",
        "title": "Kayorama: Dump All Commands"
      },
      {
        "command": "kayorama.inspectDOM",
        "title": "Kayorama: Inspect DOM (Debug)"
      }
    ],
    "configuration": {
      "title": "Kayorama Autopilot",
      "properties": {
        "kayorama.cdpPort": {
          "type": "number",
          "default": 9000,
          "description": "Port to connect to VS Code's Chrome DevTools Protocol. (Launch VS Code with --remote-debugging-port=9000)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/node": "20.2.5",
    "@types/vscode": "^1.75.0",
    "@types/ws": "^8.18.1",
    "@vscode/vsce": "^2.19.0",
    "typescript": "^5.1.3"
  }
}